{
    "version": "5.0.2.0",
    "description": "CosMOS Beginner. Brand new self-service gamehacking tool",
    "homepage": "https://www.cheathappens.com/cosmos.asp",
    "license": "Freeware",
    "url": "https://www.cheathappens.com/CoSMOS_Beg_Release_5020.zip",
    "hash": "9408e51aa3988d4a92a4fd474b5a5f96a611271f7e3db90805dab879d4f3b72b",
    "pre_install": [
        "$file = 'CoSMOS.settings'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
        "    $CONT = @('{', '\"CheckForUpdates\": false', '}')",
        "    Set-Content \"$dir\\$file\" $CONT -Encoding Ascii",
        "}"
    ],
    "shortcuts": [
        [
            "CoSMOS_Beginner.exe",
            "CosMOS Beginner"
        ]
    ],
    "persist": "CoSMOS.settings",
    "checkver": {
        "url": "https://www.cheathappens.com/cosmos_changelogbeg5.txt",
        "regex": "CoSMOS Beginner Changelog ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://www.cheathappens.com/CoSMOS_Beg_Release_$cleanVersion.zip"
    }
}
