{
    "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
    "version": "6.0.100-rc.1.21463.6",
    "homepage": "https://www.microsoft.com/net/",
    "description": ".NET is a free, cross-platform, open source developer platform for building many different types of applications.",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/Sdk/6.0.100-rc.1.21463.6/dotnet-sdk-6.0.100-rc.1.21463.6-win-x64.zip",
            "hash": "3e2b6c5693258d782d9c1e515519ba1ad7e53e42b95ce98d0581f541d170f2f9"
        },
        "32bit": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/Sdk/6.0.100-rc.1.21463.6/dotnet-sdk-6.0.100-rc.1.21463.6-win-x86.zip",
            "hash": "6540cbd0b1faa0b6e948464df10768a5abcbe0aa48238d433f2c2779e1b41f66"
        }
    },
    "bin": "dotnet.exe",
    "checkver": {
        "url": "https://raw.githubusercontent.com/dotnet/core/master/release-notes/releases-index.json",
        "jsonpath": "$..releases-index[0]",
        "regex": "\"latest-release\": \"(?<shaver>[^\"]+)\",(?s:.)*?\"latest-sdk\": \"(?<version>[^\"]+)\",",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dotnetcli.blob.core.windows.net/dotnet/Sdk/$version/dotnet-sdk-$version-win-x64.zip"
            },
            "32bit": {
                "url": "https://dotnetcli.blob.core.windows.net/dotnet/Sdk/$version/dotnet-sdk-$version-win-x86.zip"
            }
        },
        "hash": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/checksums/$matchShaver-sha.txt"
        }
    }
}
